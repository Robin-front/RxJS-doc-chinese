{
  "generator": "site",
  "title": "RxJS - Javascript library for functional reactive programming.",
  "description": "RxJS 中文文档",
  "structure": {
      "readme": "intro.md"
  },
  "plugins": [
      "anchors",
      "toggle-chapters",
      "editlink",
      "splitter",
      "github-buttons",
      "sitemap-general",
      "simple-page-toc",
      "quiz",
      "include-highlight",
      "tonic",
      "ace",
      "ga"
  ],
  "theme": "theme",
  "pluginsConfig": {
      "theme-default": {
        "showLevel": true,
        "styles": {
          "website": "build/gitbook.css"
        }
      },
      "fontSettings": {
          "theme": "white",
          "family": "sans"
      },
      "ga": {
        "token": "UA-74634898-2"
      },
      "editlink": {
          "base": "https://github.com/Robin-front/RxJS-doc-chinese/edit/master",
          "label": "编辑本页"
      },
      "github-buttons": {
          "repo": "Robin-front/RxJS-doc-chinese",
          "types": [
              "star"
          ],
          "size": "small"
      },
      "sitemap-general": {
          "prefix": "https://robin-front.gitbooks.io/rxjs-doc-chinese/content/"
      },
      "simple-page-toc": {
          "maxDepth": 3,
          "skipFirstH1": true
      }
  },
  "variables": {
      "isPdf": false
  }
}
